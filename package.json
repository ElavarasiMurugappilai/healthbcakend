{
  "name": "health-backend",
  "version": "1.0.0",
  "description": "Health tracking application with frontend and backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd Frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../Frontend && npm install",
    "build": "cd backend && npm run build && cd ../Frontend && npm run build"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react-toastify": "^4.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.34.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "react-toastify": "^11.0.5",
    "socket.io": "^4.8.1"
  }
}
